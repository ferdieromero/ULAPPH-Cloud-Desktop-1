<script>

$(function(){
  var entries = [
    { value: 'Elementary Mathematics Grade 1: Chapter I. Grade 1, Subtraction', data: 'https://lrmds.deped.gov.ph/detail/2183' }, 
    { value: 'MTB-MLE Grade 1: Bikol Week 1 Teacherâ€™s Guide', data: 'https://lrmds.deped.gov.ph/detail/3933' },   
  ];
  // setup autocomplete function pulling from entries[] array
  $('#autocomplete').autocomplete({
	lookup: entries,
	onSelect: function (suggestion) {
	  		var x = document.getElementById("scheme").value;
	  		var str = suggestion.data;
	  		var res = "";
	  		if (x == "http:") {
	  			res = str.replace("https://", "http://");
	  		}else {
	  			res = str.replace("http://", "https://");
	  		}
	        var thehtml = "<strong><a href='#' onClick=\"location.href='" + res + "'; return false;\">Open Here</a></strong> | <strong><a href='#' onClick=\"window.open('" + res + "'); return false;\">In New Tab</a></strong> | <strong><a href='#' onClick=\"window.open();return false;\">Blank Window</a></strong>";
	        $('#outputcontent').html(thehtml);
	}
  });

});

</script>
</head>
