<script>

$(function(){
  var entries = [
    { value: 'Kinder-Week-1', data: 'http://lrmds.deped.gov.ph/list/kto12/kinder/1' }, 
    { value: 'Kinder-Week-2', data: 'http://lrmds.deped.gov.ph/list/kto12/kinder/2' },   
  ];
  // setup autocomplete function pulling from entries[] array
  $('#autocomplete').autocomplete({
	lookup: entries,
	onSelect: function (suggestion) {
	  location.href = suggestion.data;
	}
  });

});

</script>
</head>
